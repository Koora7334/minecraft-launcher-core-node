{
  "name": "@xmcl/murmurhash2",
  "version": "0.0.1",
  "description": "The murmurhash2 algorithm implemented in AssemblyScript",
  "publishConfig": {
    "access": "public",
    "module": "./build/release.js"
  },
  "scripts": {
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "pnpm run asbuild:debug && pnpm run asbuild:release"
  },
  "keywords": [
    "murmurhash2",
    "webassembly"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Voxelum/minecraft-launcher-core-node.git"
  },
  "homepage": "https://github.com/Voxelum/minecraft-launcher-core-node#readme",
  "author": "cijhn@hotmail.com",
  "bugs": {
    "url": "https://github.com/Voxelum/minecraft-launcher-core-node/issues"
  },
  "license": "MIT",
  "devDependencies": {
    "assemblyscript": "^0.27.27"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    },
    "./wasm": {
      "import": "./build/release.wasm"
    }
  }
}